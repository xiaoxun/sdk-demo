{
  "env": {
    "node": true,
    "es6": true
  },
  "globals": {
    "_": true,
    "Promise": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": "airbnb/legacy",
  "rules": {
    "no-new": 0,
    "no-param-reassign": 0,
    "func-names": 0,
    "no-underscore-dangle": 0,
    "max-len": 0
  }
}